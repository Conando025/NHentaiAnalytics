{
    "name": "NHentai Analytics",
    "version": "1.0.0",
    "description": "A chrome extension to help you find the best doujinshi based on your favorites",
    "browser_action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "storage",
      "https://nhentai.net/*",
      "https://i.nhentai.net/*"
    ],
    "author": "Xwilarg",
    "background": {
      "scripts": ["js/background.js"]
    },
    "options_ui": {
      "page": "settings.html",
      "open_in_tab": false
    },
    "manifest_version": 2
}